---
import LottieAnimation from "astro-integration-lottie/Lottie.astro";

interface props{
    className:string
    loop:boolean
}

const {className,loop=true}=Astro.props
---
<div class={`${className} w-[90px]`}>
    <LottieAnimation src="/lotties/vestuario.json" autoplay="visible" loop={loop} />
</div>  